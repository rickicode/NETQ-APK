            },
            "tag": "api"
        },
        {
            "listen": "127.0.0.1",
            "port": 20002,
            "protocol": "vless",
            "settings": {
                "clients": []
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": true,
                    "path": "/tunnelvless"
                }
            },
            "tag": "VLESS"
        },
        {
            "listen": "127.0.0.1",
            "port": 20012,
            "protocol": "vless",
            "settings": {
                "clients": []
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "streamSettings": {
                "grpcSettings": {
                    "level": 0,
                    "serviceName": "tunnelvless-grpc"
                },
                "network": "grpc",
                "security": "none"
            },
            "tag": "VLESS"
        }
    ],
    "log": {
        "access": "/var/log/netq/access-vless.log",
        "error": "/var/log/netq/error-vless.log",
        "loglevel": "warning"
